{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": false,
    "border": {
      "color": false,
      "radius": false,
      "style": false,
      "width": false
    },
    "color": {
      "background": false,
      "link": false,
      "text": false,
      "custom": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false
    },
    "spacing": {
      "blockGap": null,
      "margin": true,
      "padding": true,
      "units": []
    },
    "typography": {
      "customFontSize": false,
      "dropCap": false,
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "lineHeight": false,
      "textColumns": false,
      "textDecoration": false,
      "textTransform": false,
      "writingMode": false,
      "fontSizes": []
    },
    "layout": {
      "contentSize": "800px"
    },
    "blocks": {
      "core/image": {
        "lightbox": {
          "enabled": false,
          "allowEditing": false
        }
      }
    }
  },
  "styles": {}
}